{
  "name": "sequential-promises-chain",
  "version": "1.0.0",
  "description": "Resolve promises in order",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' '__tests__/**/*.ts'",
    "pretest": "npm run lint",
    "test": "jest --forceExit"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daton89-topperblues/sequential-promises-chain.git"
  },
  "keywords": [
    "order",
    "promises"
  ],
  "author": "@topperblues Nicola Bonavita @daton89 Antonio D'angelo",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/daton89-topperblues/sequential-promises-chain/issues"
  },
  "homepage": "https://github.com/daton89-topperblues/sequential-promises-chain#readme",
  "devDependencies": {
    "@types/core-js": "^0.9.42",
    "@types/jest": "^20.0.2",
    "@types/node": "^8.0.11",
    "jest": "^20.0.4",
    "pre-commit": "^1.2.2",
    "ts-jest": "^20.0.7",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "verbose": true,
    "notify": false,
    "bail": false
  }
}
